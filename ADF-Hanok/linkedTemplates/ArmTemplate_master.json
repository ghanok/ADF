{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Hanok"
		},
		"AzureBlobStorage_hanok_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_hanok'"
		},
		"AzureDataLakeStorage_hanok_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_hanok'"
		},
		"AzureDataLakeStorage_hanok_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccounthanok.dfs.core.windows.net/"
		},
		"LS_ADLSgen2_keyvault_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccounthanok.dfs.core.windows.net/"
		},
		"LS_Keyvalut_hanok_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Hanok-keyvault.vault.azure.net/"
		},
		"LS_AzureSQL_hanok_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hanoksqlserver.database.windows.net"
		},
		"LS_AzureSQL_hanok_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "hanok_sql_database"
		},
		"LS_AzureSQL_hanok_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin123"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Hanok_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_hanok_connectionString": {
						"value": "[parameters('AzureBlobStorage_hanok_connectionString')]"
					},
					"AzureDataLakeStorage_hanok_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_hanok_accountKey')]"
					},
					"AzureDataLakeStorage_hanok_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_hanok_properties_typeProperties_url')]"
					},
					"LS_Keyvalut_hanok_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Keyvalut_hanok_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-Hanok_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSgen2_keyvault_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSgen2_keyvault_properties_typeProperties_url')]"
					},
					"LS_AzureSQL_hanok_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQL_hanok_properties_typeProperties_server')]"
					},
					"LS_AzureSQL_hanok_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQL_hanok_properties_typeProperties_database')]"
					},
					"LS_AzureSQL_hanok_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQL_hanok_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-Hanok_ArmTemplate_0')]"
			]
		}
	]
}